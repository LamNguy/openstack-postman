		{
			"name": "Credentials",
			"description": "/v3/credentials",
			"item": [
				{
					"name": "Create credential (EC2)",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"type": "text/javascript",
								"exec": "// Set the requested access string\npostman.setEnvironmentVariable(\"access_str\", \"SETME\");\n\n// Set the requested secret key\npostman.setEnvironmentVariable(\"secret_key\", \"SETME\");\n\n// Set the Tenant ID\npostman.setEnvironmentVariable(\"tenant_id\", \"{{OS_TENANT_ID}}\");\n\n// Set the User ID\npostman.setEnvironmentVariable(\"user_id\", \"{{OS_USERID}}\");\n"
							}
						}
					],
					"request": {
						"url": "http://{{IP}}:{{KEYSTONE_PORT}}/v3/credentials",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "X-Auth-Token",
								"value": "{{TOKEN}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "    {\r\n    \"credential\": {\r\n        \"blob\": \"{\\\"access\\\":\\\"{{access_str}}\\\",\\\"secret\\\":\\\"{{secret_key}}\\\"}\",\r\n        \"project_id\": \"{{tenant_id}}\",\r\n        \"type\": \"ec2\",\r\n        \"user_id\": \"{{user_id}}\"\r\n    }\r\n}"
						},
						"description": "/v3/credentials"
					},
					"response": []
				},
