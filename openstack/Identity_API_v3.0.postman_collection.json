		{
			"name": "Domains",
			"description": "/v3/domains",
			"item": [
				{
					"name": "List domains [Check]",
					"request": {
						"url": "http://{{IP}}:{{KEYSTONE_PORT}}/v3/domains?name&enabled",
						"method": "GET",
						"header": [
							{
								"key": "X-Auth-Token",
								"value": "{{TOKEN}}",
								"description": ""
							}
