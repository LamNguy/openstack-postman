								"key": "X-Auth-Token",
								"value": "{{TOKEN}}",
								"description": ""
							},
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"domain\": {\r\n        \"description\": \"{{domain_description}}\",\r\n        \"enabled\": {{domain_enabled}},\r\n        \"name\": \"{{domain_name}}\"\r\n    }\r\n}"
						},
						"description": "/v3/domains"
					},
					"response": []
				},
