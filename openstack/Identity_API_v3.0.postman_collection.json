			"item": [
				{
					"name": "Password authentication with unscoped authorization",
					"request": {
						"url": "http://{{IP}}:{{KEYSTONE_PORT}}/v3/auth/tokens",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n    \"auth\": {\n        \"identity\": {\n            \"methods\": [\n                \"password\"\n            ],\n            \"password\": {\n                \"user\": {\n                    \"name\": \"{{OS_USERNAME}}\",\n                    \"domain\": {\n                        \"id\": \"default\"\n                    },\n                    \"password\": \"{{OS_PASSWORD}}\"\n                }\n            }\n        }\n    }\n}"
						},
						"description": "/v3/auth/tokens"
					},
					"response": []
				},
				{
