							"listen": "prerequest",
							"script": {
								"type": "text/javascript",
								"exec": "// Set the Domain Description\n\npostman.setEnvironmentVariable(\"domain_description\", \"SETME\");\n\n// Enable or disable the domain\npostman.setEnvironmentVariable(\"domain_enabled\", true);\n\n// Set the Domain Name\npostman.setEnvironmentVariable(\"domain_name\", \"SETME\");"
							}
						}
					],
					"request": {
						"url": "http://{{IP}}:{{KEYSTONE_PORT}}/v3/domains",
						"method": "POST",
						"header": [
