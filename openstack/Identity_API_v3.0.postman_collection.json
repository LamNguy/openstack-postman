					"response": []
				},
				{
					"name": "Create domain [Check]",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"type": "text/javascript",
								"exec": "// Set the Domain Description\n\npostman.setEnvironmentVariable(\"domain_description\", \"Testing Domain\");\n\n// Enable or disable the domain\npostman.setEnvironmentVariable(\"domain_enabled\", true);\n\n// Set the Domain Name\npostman.setEnvironmentVariable(\"domain_name\", \"testdomain\");"
							}
						}
					],
					"request": {
						"url": "http://{{IP}}:{{KEYSTONE_PORT}}/v3/domains",
						"method": "POST",
						"header": [
							{
								"key": "X-Auth-Token",
								"value": "{{TOKEN}}",
								"description": ""
							},
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"domain\": {\r\n        \"description\": \"{{domain_description}}\",\r\n        \"enabled\": {{domain_enabled}},\r\n        \"name\": \"{{domain_name}}\"\r\n    }\r\n}"
						},
						"description": "/v3/domains"
					},
					"response": []
				}
			]
