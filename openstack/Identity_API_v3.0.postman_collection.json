		{
			"name": "Credentials",
			"description": "/v3/credentials",
			"item": [
				{
					"name": "Create credential (EC2) [Check]",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"type": "text/javascript",
								"exec": "// Set the requested access string\n//postman.setEnvironmentVariable(\"access_str\", \"SETME\");\n\n// Set the requested secret key\n//postman.setEnvironmentVariable(\"secret_key\", \"SETME\");"
							}
						}
					],
					"request": {
						"url": "http://{{IP}}:{{KEYSTONE_PORT}}/v3/credentials",
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json",
								"description": ""
							},
							{
								"key": "X-Auth-Token",
								"value": "{{TOKEN}}",
								"description": ""
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\r\n    \"credential\": {\r\n        \"blob\": \"{\\\"access\\\":\\\"{{access_str}}\\\",\\\"secret\\\":\\\"{{secret_key}}\\\"}\",\r\n        \"project_id\": \"{{OS_TENANT_ID}}\",\r\n        \"type\": \"ec2\",\r\n        \"user_id\": \"{{OS_USERID}}\"\r\n    }\r\n}"
